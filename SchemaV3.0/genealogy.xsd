<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:element name="family">
    <xs:complexType mixed="true">
      <xs:attribute name="surname" use="required"/>
      <xs:attribute name="basePersonId" use="required"/>
      <xs:attribute name="baseMarriageId" use="required"/>
      <xs:attribute name="email" use="required"/>
      <xs:attribute name="dataPath" use="required"/>
      <xs:attribute name="outputPathTXT"/>
      <xs:attribute name="outputPathHTML"/>
      <xs:attribute name="outputPathWP"/>
      <xs:attribute name="formsBaseUrlFile"/>
      <xs:attribute name="formsBaseUrlWeb"/>
      <xs:attribute name="formsCounterFgs"/>
      <xs:attribute name="formsCounterPerInfo"/>
      <xs:attribute name="formsCounterPerIndex"/>
      <xs:attribute name="formsCounterPhoto"/>
      <xs:attribute name="formsCounterPhotoIndex"/>
      <xs:attribute name="formsCounterRef"/>
      <xs:attribute name="formsCounterRefIndex"/>
      <xs:attribute name="formsCounterHome"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="marriage">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="marriageInfo"/>
        <xs:element minOccurs="0" ref="termination"/>
      </xs:sequence>
      <xs:attribute name="marriageId" use="required"/>
      <xs:attribute name="husbandPersonId" use="required"/>
      <xs:attribute name="wifePersonId" use="required"/>
      <xs:attribute name="version" use="required"/>
      <xs:attribute name="inLine">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="true"/>
            <xs:enumeration value="false"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="sealToSpouseInfo">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="info"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="proxy"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="marriageInfo">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="info"/>
        <xs:element minOccurs="0" ref="sealToSpouseInfo"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="termination">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="date"/>
      </xs:sequence>
      <xs:attribute name="reason">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="Divorce"/>
            <xs:enumeration value="Seperation"/>
            <xs:enumeration value="DeathOfHusband"/>
            <xs:enumeration value="DeathOfWife"/>
            <xs:enumeration value="NeverMarried"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="marriages">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="marriage"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="person">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="personIdentification" maxOccurs="1" minOccurs="1"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="profession"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="religion"/>
        <xs:element minOccurs="0" maxOccurs="1" ref="birthInfo"/>
        <xs:element minOccurs="0" maxOccurs="1" ref="chrInfo"/>
        <xs:element minOccurs="0" maxOccurs="1" ref="deathInfo"/>
        <xs:element minOccurs="0" maxOccurs="1" ref="burialInfo"/>
        <xs:element minOccurs="0" maxOccurs="1" ref="ldsInfo"/>
        <xs:element minOccurs="0" maxOccurs="1" ref="eventGroup"/>
      </xs:sequence>
      <xs:attribute name="version" use="required"/>
      <xs:attribute name="personId" use="required"/>
      <xs:attribute name="parentId"/>
      <xs:attribute name="relationshipToPrimary"/>
      <xs:attribute name="adopted">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="Yes"/>
            <xs:enumeration value="No"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="afn"/>
      <xs:attribute name="ssn"/>
      <xs:attribute name="gender" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="MALE"/>
            <xs:enumeration value="FEMALE"/>
            <xs:enumeration value="UNKNOWN"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="living">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="Yes"/>
            <xs:enumeration value="No"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="eventGroup">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="event"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="event">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="eventTitle"/>
        <xs:element minOccurs="0" ref="date"/>
        <xs:element minOccurs="0" ref="place"/>
        <xs:element ref="eventDescription"/>
      </xs:sequence>
      <xs:attribute name="tag" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="BornD"/>
            <xs:enumeration value="BornP"/>
            <xs:enumeration value="Born"/>
            <xs:enumeration value="ChrD"/>
            <xs:enumeration value="ChrP"/>
            <xs:enumeration value="Chr"/>
            <xs:enumeration value="DiedCause"/>
            <xs:enumeration value="DiedD"/>
            <xs:enumeration value="DiedP"/>
            <xs:enumeration value="Died"/>
            <xs:enumeration value="BapD"/>
            <xs:enumeration value="BapP"/>
            <xs:enumeration value="Bap"/>
            <xs:enumeration value="EndD"/>
            <xs:enumeration value="EndP"/>
            <xs:enumeration value="End"/>
            <xs:enumeration value="BurD"/>
            <xs:enumeration value="BurP"/>
            <xs:enumeration value="Bur"/>
            <xs:enumeration value="SToPD"/>
            <xs:enumeration value="SToPP"/>
            <xs:enumeration value="SToP"/>
            <xs:enumeration value="Title"/>
            <xs:enumeration value="Profession"/>
            <xs:enumeration value="Religion"/>
            <xs:enumeration value="LinkCToFather"/>
            <xs:enumeration value="LinkCToMother"/>
            <xs:enumeration value="LinkFToChildren"/>
            <xs:enumeration value="LinkMToChildren"/>
            <xs:enumeration value="Location"/>
            <xs:enumeration value="Emigration"/>
            <xs:enumeration value="Name"/>
            <xs:enumeration value="Headstone"/>
            <xs:enumeration value="WillAndTestament"/>
            <xs:enumeration value="AgeInYear"/>
            <xs:enumeration value="CemeteryName"/>
            <xs:enumeration value="CemeteryPlot"/>
            <xs:enumeration value="LinkPToDescendants"/>
            <xs:enumeration value="Home"/>
            <xs:enumeration value="OwnerOf"/>
            <xs:enumeration value="BapTemple"/>
            <xs:enumeration value="BapProxy"/>
            <xs:enumeration value="EndTemple"/>
            <xs:enumeration value="EndProxy"/>
            <xs:enumeration value="Personality"/>
            <xs:enumeration value="PersonComment"/>
            <xs:enumeration value="Hobbies"/>
            <xs:enumeration value="Education"/>
            <xs:enumeration value="Siblings"/>
            <xs:enumeration value="EditorNotes"/>
            <xs:enumeration value="TimeLine"/>
            <xs:enumeration value="BirthStats"/>
            <xs:enumeration value="Trivia"/>
            <xs:enumeration value="Obituary"/>
            <xs:enumeration value="Marriage"/>
            <xs:enumeration value="Uncategorized"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="eventTitle">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="paragraph"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="eventDescription">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="paragraph"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="birthInfo">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="info"/>
      </xs:sequence>
      <xs:attribute name="hospital"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="chrInfo">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="info"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="deathInfo">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="info"/>
      </xs:sequence>
      <xs:attribute name="cause"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="personIdentification">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="personName"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="nickName"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="moniker"/>
      </xs:sequence>
      <xs:attribute name="royalEnumeration"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="nickName">
    <xs:complexType mixed="true">
      <xs:attribute name="name"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="moniker" type="xs:string"/>
  <xs:element name="religion">
    <xs:complexType mixed="true">
      <xs:attribute name="name"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="profession">
    <xs:complexType mixed="true">
      <xs:attribute name="name"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="burialInfo">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="info"/>
      </xs:sequence>
      <xs:attribute name="cemeteryName"/>
      <xs:attribute name="cemeteryPlotAddress"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="ldsInfo">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="baptismInfo"/>
        <xs:element minOccurs="0" ref="endowmentInfo"/>
        <xs:element minOccurs="0" ref="sealToParentsInfo"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="endowmentInfo">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="info"/>
        <xs:element minOccurs="0" ref="proxy"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="baptismInfo">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="info"/>
        <xs:element minOccurs="0" ref="proxy"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="sealToParentsInfo">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="info"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="proxy"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="persons">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="person"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="photo">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="description"/>
        <xs:element ref="fileList"/>
        <xs:element ref="source"/>
        <xs:element minOccurs="0" ref="seeAlso"/>
      </xs:sequence>
      <xs:attribute name="photoId" use="required"/>
      <xs:attribute name="access">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="PUBLIC"/>
            <xs:enumeration value="PRIVATE"/>
            <xs:enumeration value="COPYRIGHTED"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="description">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="paragraph"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="fileList">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="file"/>
      </xs:sequence>
      <xs:attribute name="package" use="required"/>
      <xs:attribute name="baseFileName" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="file">
    <xs:complexType mixed="true">
      <xs:attribute name="type" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="JPG"/>
            <xs:enumeration value="PNG"/>
            <xs:enumeration value="GIF"/>
            <xs:enumeration value="BMP"/>
            <xs:enumeration value="TIFF"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="quality" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="Lowest"/>
            <xs:enumeration value="Low"/>
            <xs:enumeration value="Medium"/>
            <xs:enumeration value="High"/>
            <xs:enumeration value="Highest"/>
            <xs:enumeration value="Thumbnail"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="size" use="required"/>
      <xs:attribute name="uniqueId" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="source">
    <xs:complexType>
      <xs:choice>
        <xs:element ref="singleton"/>
        <xs:element ref="publishedIn"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="singleton">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="date"/>
        <xs:element minOccurs="0" ref="place"/>
        <xs:element minOccurs="0" ref="caption"/>
        <xs:element minOccurs="0" ref="photographer"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="owner"/>
        <xs:element minOccurs="0" ref="transcription"/>
        <xs:element minOccurs="0" ref="commentary"/>
        <xs:element minOccurs="0" ref="tagGroup"/>
      </xs:sequence>
      <xs:attribute name="mediaType">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="DigitalPhoto"/>
            <xs:enumeration value="Painting"/>
            <xs:enumeration value="Photo"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="caption">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="paragraph"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="photographer">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="personName"/>
        <xs:element minOccurs="0" ref="studioName"/>
      </xs:sequence>
      <xs:attribute name="personId"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="studioName" type="xs:string"/>
  <xs:element name="owner">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="personName"/>
        <xs:element minOccurs="0" ref="asOf"/>
        <xs:element minOccurs="0" ref="address"/>
      </xs:sequence>
      <xs:attribute name="personId"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="asOf">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="date"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="address">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="paragraph"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="commentary">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="paragraph"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="transcription">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="paragraph"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="publishedIn">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="referenceEntryId"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="photos">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="photo"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="reference">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="citation"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="entry"/>
      </xs:sequence>
      <xs:attribute name="referenceId" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="citation">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="authority"/>
        <xs:element ref="place"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="dateRange"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="classification"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="authority">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="title"/>
        <xs:element minOccurs="0" ref="author"/>
        <xs:element minOccurs="0" ref="publisher"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="location"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="publisher">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="company"/>
        <xs:element minOccurs="0" ref="date"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="location">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="institution"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="callNumber"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="chapterNumbers" type="xs:string"/>
  <xs:element name="pageNumbers" type="xs:string"/>
  <xs:element name="volumeNumbers" type="xs:string"/>
  <xs:element name="header">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="volumeNumbers"/>
        <xs:element minOccurs="0" ref="chapterNumbers"/>
        <xs:element minOccurs="0" ref="pageNumbers"/>
        <xs:element minOccurs="0" ref="entryNumbers"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="paragraph"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="institution">
    <xs:complexType mixed="true">
      <xs:attribute name="name" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="company">
    <xs:complexType mixed="true">
      <xs:attribute name="name" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="callNumber">
    <xs:complexType mixed="true">
      <xs:attribute name="name" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="author">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="personName"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="groupName"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="comment">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="paragraph"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="body">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded">
         <xs:element ref="paragraph"/>
         <xs:element ref="table"/>
<!--
        <xs:sequence minOccurs="0" maxOccurs="unbounded">
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="table"/>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="paragraph"/>
        </xs:sequence>
-->
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="entryNumbers" type="xs:string"/>
  <xs:element name="entryTitle">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="paragraph"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="title">
    <xs:complexType mixed="true">
      <xs:attribute name="name"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="groupName">
    <xs:complexType mixed="true">
      <xs:attribute name="name" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="entry">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="entryTitle"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="place"/>
        <xs:element minOccurs="0" ref="header"/>
        <xs:element minOccurs="0" ref="body"/>
        <xs:element minOccurs="0" ref="comment"/>
        <xs:element minOccurs="0" ref="tagGroup"/>
        <xs:element minOccurs="0" ref="seeAlso"/>
      </xs:sequence>
      <xs:attribute name="access" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="PUBLIC"/>
            <xs:enumeration value="PRIVATE"/>
            <xs:enumeration value="COPYRIGHTED"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="entryId" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="classification">
    <xs:complexType mixed="true">
      <xs:attribute name="type">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="COMPILED"/>
            <xs:enumeration value="ORIGINAL"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="subType">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="CivilRegistry"/>
            <xs:enumeration value="Census"/>
            <xs:enumeration value="Newspaper"/>
            <xs:enumeration value="Gazetteer"/>
            <xs:enumeration value="PersonalLetter"/>
            <xs:enumeration value="LocationHistory"/>
            <xs:enumeration value="FamilyHistory"/>
            <xs:enumeration value="GeneralHistory"/>
            <xs:enumeration value="PersonalExperience"/>
            <xs:enumeration value="LandRecord"/>
            <xs:enumeration value="CemeteryInscriptions"/>
            <xs:enumeration value="ChurchRecord"/>
            <xs:enumeration value="ProbateRecord"/>
            <xs:enumeration value="CivilVitalRecord"/>
            <xs:enumeration value="EmigrationRecord"/>
            <xs:enumeration value="CemeteryRecords"/>
            <xs:enumeration value="MilitaryRecords"/>
            <xs:enumeration value="PhotoAlbum"/>
            <xs:enumeration value="Directory"/>
            <xs:enumeration value="AutoBiography"/>
            <xs:enumeration value="Interview"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="references">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="reference"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="place">
